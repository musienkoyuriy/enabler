{
  "name": "enabler",
  "version": "2.10.0",
  "description": "Accessibility analyzer for your frontend",
  "main": "./lib/cli.js",
  "scripts": {
    "start": "npm run-s build",
    "clean": "rimraf lib",
    "tsc": "tsc -p src",
    "build": "npm run clean && npm run tsc",
    "test": "echo \"Error: no test specified\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "cleaninstall": "rimraf ./node_modules && npm cache verify && npm i",
    "demo:html": "node lib/cli.js -p fixtures",
    "demo:ng": "node lib/cli.js -p fixtures --ng",
    "demo:vue": "node lib/cli.js -p fixtures --vue",
    "demo:react": "node lib/cli.js -p fixtures --react",
    "prepublish": "npm run build",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*.js",
    "bin/enabler"
  ],
  "bin": {
    "enabler": "./bin/enabler"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/musienkoyuriy/enabler.git"
  },
  "keywords": [
    "wai-aria",
    "wcag",
    "screen-reader",
    "enabler",
    "ngx-a11y",
    "vue-a11y",
    "accessibility",
    "semantic",
    "WCAG",
    "WAI",
    "ADA Compliance"
  ],
  "author": {
    "name": "Yuriy Musienko",
    "email": "musienkoyuriy@gmail.com",
    "url": "https://twitter.com/musienko_yuriy"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/musienkoyuriy/enabler/issues"
  },
  "homepage": "https://github.com/musienkoyuriy/enabler#readme",
  "dependencies": {
    "@types/cheerio": "0.22.26",
    "@types/esprima": "4.0.2",
    "@types/node": "14.14.33",
    "aria-query": "4.2.2",
    "chalk": "4.1.0",
    "cheerio": "^1.0.0-rc.5",
    "commander": "7.1.0",
    "escodegen": "2.0.0",
    "escodegen-jsx": "0.1.0-1.4.2dev",
    "esprima": "4.0.1",
    "glob": "^7.1.6",
    "is-react": "1.5.4",
    "module-alias": "2.2.2",
    "tslib": "2.1.0",
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "husky": "^4.3.8",
    "prettier": "2.2.1",
    "rimraf": "3.0.2"
  },
  "eslintIgnore": [
    "fixtures/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm run build"
    }
  },
  "_moduleAliases": {
    "@models": "dist/lib/models"
  }
}